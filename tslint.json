{
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "array-type": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "member-access": false,
        "no-empty-interface": false,
        "object-literal-sort-keys": false,
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": false
    }
}